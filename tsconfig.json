{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": [
      "@cloudflare/workers-types"
    ],
    "lib": [
      "ESNext"
    ],
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext"
  }
}